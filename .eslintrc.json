{
	"env": {
		"browser": true,
		"es2021": true,
		"node": true
	},
	"extends": ["airbnb-base"],
	"parserOptions": {
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"rules": {
		"no-tabs": "off",
		"indent": "off",
		"quotes": ["error", "double"],
		"comma-dangle": "off",
		"no-console": "off"
	}
}
